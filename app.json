{
  "expo": {
    "name": "CanIEat",
    "slug": "CanIeat",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/icon.png",
    "scheme": "canieat",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "description": "AI-powered food safety guide for pregnant women based on UK government medical guidelines",
    "keywords": [
      "pregnancy",
      "food safety",
      "health",
      "nutrition",
      "AI",
      "medical advice",
      "UK guidelines"
    ],
    "privacy": "public",
    "platforms": [
      "ios",
      "android"
    ],
    "githubUrl": "https://github.com/olgagorskikh/canieat",
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.grsdev.canieat",
      "buildNumber": "20",
      "infoPlist": {
        "NSHealthShareUsageDescription": "This app provides food safety guidance for pregnant women based on UK medical guidelines.",
        "NSHealthUpdateUsageDescription": "This app provides food safety guidance for pregnant women based on UK medical guidelines.",
        "ITSAppUsesNonExemptEncryption": false
      }
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/adaptive-icon.png",
        "backgroundColor": "#ffffff"
      },
      "package": "com.grsdev.canieat",
      "versionCode": 3
    },
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./assets/images/favicon.png"
    },
    "plugins": [
      "expo-router",
      [
        "expo-splash-screen",
        {
          "image": "./assets/images/splash-icon.png",
          "imageWidth": 200,
          "resizeMode": "contain",
          "backgroundColor": "#ffffff"
        }
      ],
      "expo-font"
    ],
    "experiments": {
      "typedRoutes": true
    },
    "extra": {
      "router": {},
      "eas": {
        "projectId": "c0c24f5a-230b-4308-b776-2debcac2369d"
      }
    }
  }
}
